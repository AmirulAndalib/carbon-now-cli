{
  "name": "carbon-now-cli",
  "version": "1.4.0",
  "description": "Beautiful images of your code — from right inside your terminal.",
  "license": "MIT",
  "repository": "mixn/carbon-now-cli",
  "type": "module",
  "author": {
    "name": "Miloš Sutanovac",
    "email": "sutanovac.milos@gmail.com",
    "url": "https://mixn.io"
  },
  "exports": "./cli.js",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "ts-node --esm --files ./cli.ts test/test-dummies/_unfold.js --start 3 --end 100 --open --location ~/Desktop --target --interactive --preset twitter --from-clipboard --headless",
    "watch": "nodemon --exec ts-node --esm --files ./cli.ts test/test-dummies/_unfold.js --start 3 --end 100 --open --location ~/Desktop --target foo.jpg --interactive --preset twitter --from-clipboard --headless",
    "dev:noflags": "ts-node --esm --files ./cli.ts test/test-dummies/_unfold.js",
    "build": "tsc",
    "run:build": "node dist/cli.js test/test-dummies/_unfold.js",
    "buildrun": "npm run build && npm run run:build",
    "prettier": "prettier ./**/* --check",
    "test:unit": "jest ./test/ --testPathIgnorePatterns ./test/e2e/ --verbose --coverage",
    "test:e2e": "jest ./test/e2e/index.test.ts --runInBand --verbose --testTimeout 180000 --coverage",
    "test": "npm run test:unit && npm run test:e2e",
    "prepare": "husky install"
  },
  "files": [
    "cli.js",
    "src"
  ],
  "keywords": [
    "cli",
    "cli-app",
    "carbon-now",
    "carbon-now-sh",
    "carbon-now-sh-cli",
    "carbon",
    "screenshot",
    "beautiful"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "clipboard-sys": "^1.2.0",
    "clipboardy": "^2.3.0",
    "file-exists": "^5.0.1",
    "file-extension": "^4.0.5",
    "get-stdin": "^8.0.0",
    "inquirer": "^6.5.2",
    "jsonfile": "^6.1.0",
    "listr2": "^6.2.0",
    "lodash": "^4.17.21",
    "meow": "^9.0.0",
    "nanoid": "^3.3.6",
    "open": "^8.4.2",
    "query-string": "^7.1.3",
    "tempy": "^1.0.1",
    "terminal-image": "^1.2.1",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@playwright/test": "^1.32.3",
    "@types/child-process-promise": "^2.2.2",
    "@types/file-exists": "^5.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash": "^4.14.191",
    "@types/mock-fs": "^4.13.1",
    "@types/update-notifier": "^6.0.2",
    "child-process-promise": "^2.2.1",
    "del": "^6.1.1",
    "execa": "^5.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mock-fs": "^5.2.0",
    "mock-os": "^1.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  }
}
