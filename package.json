{
	"name": "carbon-now-cli",
	"version": "1.4.0",
	"description": "Beautiful images of your code — from right inside your terminal.",
	"license": "MIT",
	"repository": "mixn/carbon-now-cli",
	"type": "module",
	"author": {
		"name": "Miloš Sutanovac",
		"email": "sutanovac.milos@gmail.com",
		"url": "https://mixn.io"
	},
	"exports": "./cli.js",
	"engines": {
		"node": ">=14.16"
	},
	"scripts": {
		"dev": "nodemon cli.ts test/test-dummies/_unfold.js",
		"once": "ts-node --esm cli.ts test/test-dummies/_unfold.js --start 3 --end 100 --open --location ~/Desktop --interactive --preset twitter --from-clipboard --headless",
		"build": "trash dist/ && tsc",
		"run:build": "node dist/cli.js test/test-dummies/_unfold.js",
		"buildrun": "npm run build && npm run run:build",
		"prettier": "prettier ./**/* --check",
		"test": "jest",
		"prepare": "husky install"
	},
	"files": [
		"cli.js",
		"src"
	],
	"keywords": [
		"cli",
		"cli-app",
		"carbon-now",
		"carbon-now-sh",
		"carbon-now-sh-cli",
		"carbon",
		"screenshot",
		"beautiful"
	],
	"dependencies": {
		"chalk": "^4.1.2",
		"clipboardy": "^2.3.0",
		"del": "^6.1.1",
		"file-exists": "^5.0.1",
		"file-extension": "^4.0.5",
		"get-stdin": "^9.0.0",
		"globby": "^8.0.1",
		"inquirer": "^6.2.1",
		"jsonfile": "^6.1.0",
		"listr": "^0.14.3",
		"lodash": "^4.17.21",
		"meow": "^11.0.0",
		"nanoid": "^2.0.0",
		"opn": "^5.4.0",
		"puppeteer": "^1.11.0",
		"query-string": "^6.2.0",
		"tempy": "^0.2.1",
		"terminal-image": "^0.1.2",
		"update-notifier": "^2.5.0"
	},
	"devDependencies": {
		"@jest/globals": "^29.5.0",
		"@types/file-exists": "^5.0.0",
		"@types/jest": "^29.5.0",
		"@types/jsonfile": "^6.1.1",
		"@types/lodash": "^4.14.191",
		"child-process-promise": "^2.2.1",
		"execa": "^5.1.1",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.5",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "~4.9.5"
	}
}
